---
layout: 'layout.njk'
lang: 'en'

pagination:
  data: pages
  size: 1
  alias: sanityData

permalink:
    build: "/{{sanityData.slug.current}}/"
    serverless: '/dynamic'
---



{% for pagesData in pages %}
   {% if pagesData.slug.current == sanityData.slug.current %}
      {% for blocks in pagesData.pageContent %}
         {% if blocks._type == "hero" %}
            {% include '_hero.njk' %}
         {% endif %}

         {% if blocks.component == "event" %}
            {% include '_event.njk' %}
         {% endif %}

         {% if blocks.component == "features" %}
            {% include '_features.njk' %}
         {% endif %}

         {% if blocks.component == "speakers" %}
            {% include '_speakers.njk' %}
         {% endif %}

         {% if blocks.component == "schedule" %}
            {% include '_schedule-btn.njk' %}
         {% endif %}

         {% if blocks.component == "full-ticket" %}
            {% include '_full-ticket.njk' %}
         {% endif %}

         {% if blocks.component == "discussions" %}
            {% include '_discussions.njk' %}
         {% endif %}

         {% if blocks.component == "follow-us" %}
            {% include '_follow-us.njk' %}
         {% endif %}

         {% if blocks.component == "bars" %}
            {% include '_bars.njk' %}
         {% endif %}
      {% endfor %}
   {% endif %}
{% endfor %}

